                    <ui:Toggle label="#showResults" name="ShowResultsPanel" tabindex="6" value="true" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="Row2" style="flex-direction: row; padding-top: 63px;">
                <ui:Button text="#done" display-tooltip-when-elided="true" name="DoneButton" tooltip="Back to menu" tabindex="7" style="width: 236px; height: 110px; font-size: 40px;" />
